{
    "compilerOptions": {
        "lib": ["es2015"],
        "jsx": "react",
        "esModuleInterop": true,
        "baseUrl": ".",
        "paths": {
            "@constants/*": ["src/constants/*"],
            "@controls/*": ["src/controls/*"],
            "@data/*": ["src/data/*"],
            "@models/*": ["src/models/*"],
            "@navigation/*": ["src/navigation/*"],
            "@pages/*": ["src/pages/*"],
            "@redux/*": ["src/redux/*"],
            "@utils/*": ["src/utils/*"]
        }
    }
}
